{
    "name": "givereferral-api",
    "profile": "up",
    "regions": [
        "us-west-2"
      ],
      "lambda": {
        "memory": 128
      },
      "proxy": {
        "command": "krakend run --config ./krakend.json"
      },
      "stages": {
        "production": {
          "domain": "api.givereferral.com",
          "lambda": {
            "warm": true
          }
        },
        "staging": {
          "domain": "api-staging.givereferral.com"
        }
      }
}