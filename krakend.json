{
    "version": 2,
    "name": "givereferral",
    "cache_ttl": "3600s",
    "timeout": "3s",
    "extra_config": {
      "github_com/devopsfaith/krakend-gologging": {
        "level":  "DEBUG",
        "prefix": "[KRAKEND]",
        "syslog": false,
        "stdout": true
      },
      "github_com/devopsfaith/krakend-cors": {
        "allow_origins": [ "https://referrers.giverferral.com", "https://providers.givereferral.com", "https://signup.givereferral.com" ],
        "allow_methods": [ "OPTIONS", "POST", "GET", "PUT", "PATCH", "DELETE", "HEAD" ],
        "allow_headers": [ "Origin", "Authorization", "Content-Type" ],
        "expose_headers": [ "Content-Length" ],
        "max_age": "12h"
      }
    },
    "endpoints": [
      {
        "endpoint": "/clients",
        "method": "POST",
        "output_encoding": "json",
        "concurrent_calls": 1,
        "extra_config": {},
        "querystring_params": [
          "tenant"
        ],
        "backend": [
          {
            "host": [
              "https://api-referrers.givereferral.com"
            ],
            "url_pattern": "/clients",
            "encoding": "json",
            "extra_config": {},
            "sd": "static"
          }
        ]
      }
    ]
}
